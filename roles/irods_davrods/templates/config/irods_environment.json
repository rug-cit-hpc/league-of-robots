{
    "irods_client_server_negotiation": "request_server_negotiation",
    "irods_client_server_policy": "{{ ir_client_server_policy }}",
    "irods_connection_pool_refresh_time_in_seconds": 300, 
    "irods_cwd": "/{{ ir_zone }}/home/rods",
    "irods_default_hash_scheme": "SHA256",
    "irods_default_number_of_transfer_threads": 4,
    "irods_default_resource": "{{ ir_default_res }}",
    "irods_encryption_algorithm": "AES-256-CBC",
    "irods_encryption_key_size": 32,
    "irods_encryption_num_hash_rounds": 16,
    "irods_encryption_salt_size": 8,
    "irods_home": "/{{ ir_zone }}/home/rods",
    "irods_host": "will_be_overwritten",
    "irods_match_hash_policy": "compatible",
    "irods_maximum_size_for_single_buffer_in_megabytes": 32,
    "irods_port": 1247,
    "irods_server_control_plane_encryption_algorithm": "AES-256-CBC",
    "irods_server_control_plane_encryption_num_hash_rounds": 16,
    "irods_server_control_plane_key": "{{ ir_ctrl_plane_key }}",
    "irods_server_control_plane_port": 1248,
    "irods_ssl_certificate_chain_file": "/etc/irods/{{ ir_ssl_certificate_chain_file }}", 
    "irods_ssl_certificate_key_file": "/etc/irods/{{ ir_ssl_certificate_key_file }}", 
    "irods_ssl_dh_params_file": "/etc/irods/{{ ir_ssl_dh_params_file }}", 
    "irods_ssl_verify_server": "cert",
    "irods_transfer_buffer_size_for_parallel_transfer_in_megabytes": 4,
    "irods_user_name": "{{ ir_admin_name }}",
    "irods_zone_name": "{{ ir_zone }}",
    "schema_name": "irods_environment",
    "schema_version": "v3"
}
